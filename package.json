{
  "name": "mcp-playground",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "calculator": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "test": "echo \"No tests specified\" && exit 0",
    "dev": "tsc -w",
    "example": "tsx src/example.ts",
    "start:mcp": "node dist/server/mcpServer.js",
    "start:api": "node dist/server/api.js",
    "start:all": "npm run build && concurrently \"npm run start:mcp\" \"sleep 2 && npm run start:api\""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "server"
  ],
