    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@langchain/community": "^0.0.15",
    "@langchain/core": "^0.1.63",
    "@langchain/openai": "^0.0.14",
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@types/dotenv": "^6.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "langchain": "^0.0.200",
